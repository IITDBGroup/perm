else
  ac_cv_prog_gnu_ld=no
fi
fi
echo "$as_me:$LINENO: result: $ac_cv_prog_gnu_ld" >&5
echo "${ECHO_T}$ac_cv_prog_gnu_ld" >&6
with_gnu_ld=$ac_cv_prog_gnu_ld




