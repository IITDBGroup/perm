#, c-format
msgid "  -t, --table=TABLE         cluster specific table only\n"
msgstr "  -t, --table=TABLE         met cette seule table en cluster\n"

#: clusterdb.c:232
#: reindexdb.c:318
#, c-format
msgid "  -q, --quiet               don't write any messages\n"
msgstr "  -q, --quiet               n'écrit aucun message\n"

