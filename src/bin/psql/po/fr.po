#: sql_help.h:102
msgid ""
"ALTER TEXT SEARCH DICTIONARY name (\n"
"    option [ = value ] [, ... ]\n"
")\n"
"ALTER TEXT SEARCH DICTIONARY name RENAME TO newname\n"
"ALTER TEXT SEARCH DICTIONARY name OWNER TO newowner"
