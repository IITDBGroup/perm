msgid "change a database"
msgstr "更改一個資料庫"

#: sql_help.h:38
msgid ""
"ALTER DATABASE name [ [ WITH ] option [ ... ] ]\n"
"\n"
"where option can be:\n"
"\n"
"    CONNECTION LIMIT connlimit\n"
"\n"
"ALTER DATABASE name RENAME TO newname\n"
"\n"
"ALTER DATABASE name OWNER TO new_owner\n"
"\n"
