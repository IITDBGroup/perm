"    [ LIMIT { count | ALL } ]\n"
"    [ OFFSET start ]\n"
"    [ FOR { UPDATE | SHARE } [ OF table_name [, ...] ] [ NOWAIT ] [...] ]"
msgstr ""
"SELECT [ ALL | DISTINCT [ ON ( expressão [, ...] ) ] ]\n"
"    * | expressão [ AS nome_saída ] [, ...]\n"
"    INTO [ TEMPORARY | TEMP ] [ TABLE ] nova_tabela\n"
"    [ FROM item_de [, ...] ]\n"
"    [ WHERE condição ]\n"
"    [ GROUP BY expressão [, ...] ]\n"
"    [ HAVING condição [, ...] ]\n"
