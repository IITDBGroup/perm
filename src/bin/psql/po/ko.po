msgstr "  \\cd [DIR]      현재 작업 디렉토리를 바꿈\n"

#: help.c:177
#, c-format
msgid "  \\copyright     show PostgreSQL usage and distribution terms\n"
msgstr "  \\copyright     PostgreSQL 사용 배포 규약을 보여줌\n"

#: help.c:178
#, c-format
msgid ""
"  \\encoding [ENCODING]\n"
"                 show or set client encoding\n"
msgstr ""
