msgid "  -l, --list               print summarized TOC of the archive\n"
msgstr ""
"  -l, --list                    vypíš sumarizovanú tabuľku obsahu (TOC) \n"
"                                archívu\n"

#: pg_restore.c:368
msgid "  -a, --data-only          restore only the data, no schema\n"
msgstr "  -a, --data-only               obnov iba dáta, schému nie\n"

