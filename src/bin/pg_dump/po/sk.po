msgid "  -C, --create             issue commands to create the database\n"
msgstr "  -C, --create                  vydaj príkazy pre vytvorenie databázy\n"

#: pg_restore.c:371
msgid "  -I, --index=NAME         restore named index\n"
msgstr "  -I, --index=NÁZOV             obnov uvedený index\n"

