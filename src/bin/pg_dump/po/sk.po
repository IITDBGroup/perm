#, c-format
msgid "  %s [OPTION]... [FILE]\n"
msgstr "  %s [VOĽBY]... [SÚBOR]\n"

#: pg_restore.c:358
msgid "  -d, --dbname=NAME        output database name\n"
msgstr "  -d, --dbname=NÁZOV            názov výstupnej databázy\n"

