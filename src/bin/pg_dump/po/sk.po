#, c-format
msgid "invalid argument string (%s) for trigger \"%s\" on table \"%s\"\n"
msgstr "neplatný reťazec argumentov (%s) pre trigger \"%s\" tabuľky \"%s\"\n"

#: pg_dump.c:7622
#, c-format
msgid ""
"query to get rule \"%s\" for table \"%s\" failed: wrong number of rows "
